{
  "private": true,
  "scripts": {
    "build:sass": "node-sass style/main.scss -o _site/style",
    "build:site": "eleventy",
    "watch:sass": "npm run build:sass -- -w",
    "watch:site": "eleventy --serve",
    "prestart": "run-p build:*",
    "start": "run-p watch:*",
    "clean": "rm -rf _site"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5"
  }
}
